{
  "name": "kingtest",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./src/build && babel ./src/. --out-dir ./src/build --copy-files",
    "db-seed:test": "sequelize db:seed:all",
    "db-migrate": "sequelize db:migrate --env production",
    "db-migrate:dev": "sequelize db:migrate --env development",
    "db-migrate:test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "prettier-eslint  '**/*.js' --write",
    "start": "npm run db-migrate && npm run build && node ./src/build/index.js",
    "start:dev": "export NODE_ENV=development && nodemon --exec babel-node src/index.js",
    "docker:start": "npm run db-migrate:dev && npm run start:dev",
    "test": "export NODE_ENV=test && npm run db-migrate:test && npm run db-seed:test && nyc --reporter=html --reporter=text mocha --require @babel/register ./src/test/*.js ./src/test/unit-tests/*.js --timeout 60000 --exit",
    "cover": "nyc --reporter=html --reporter=text npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/polyfill": "^7.12.1",
    "@hapi/joi": "^17.1.1",
    "@passport-next/passport-mocked": "^1.3.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.54.0",
    "babel-watch": "^7.7.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.37.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "ejs": "^3.1.9",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "methods": "^1.1.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "redis": "^2.8.0",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.0",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^4.6.3",
    "underscore": "^1.13.6",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/node": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/register": "^7.21.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "sinon": "^15.1.0"
  }
}
